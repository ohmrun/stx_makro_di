{
  "build" : {
    "-D" : ["no-deprecation-warnings"],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "tink_priority",
      "tink_core",
      "tink_macro",
  
  
      "stx_pico",
      "stx_nano",
      "stx_ds",
      "stx_test",
      "stx_log",
      "stx_pkg",
      "stx_test",
      "stx_makro",
      "stx_makro_di"
    ],
    "unit" : {
      "--macro" : [
        "include('stx.makro.di',true)",
        "include('stx.makro.Di',true)"
      ]
    },
    "listen" : {
      "--connect" : "7070" 
    },
    "test" : {
      "-cp" : "src/test/haxe",
      "-main" : "stx.makro.di.Test",
      "interp" : {
        "--interp" : true
      },
      "--times" : true,
      "--verbose" : true
    }
  }
}